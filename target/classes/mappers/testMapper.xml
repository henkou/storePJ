<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.example.store.dao.TestDAO">

    <select id="selectAllTest" resultType="org.example.store.dto.TestDTO">
        select username, password, etc from T_test
    </select>

<!--    <select id="selectTest" parameterType="String" resultType="org.example.store.dto.TestDTO">-->

<!--    </select>-->

    <insert id="insertTest" parameterType="org.example.store.dto.TestDTO">
        insert into T_test
        values (#{username},#{password},#{etc})
    </insert>

    <update id="updateTest" parameterType="org.example.store.dto.TestDTO">
        update T_test set
        username = #{username},
        password = #{password},
        etc = #{etc},
        where username = #{username}
    </update>

    <delete id="deleteTest" parameterType="String">
        delete from T_test
        where username = #{username}
    </delete>

</mapper>